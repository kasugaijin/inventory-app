<p style="color: green"><%= notice %></p>

<h2>Your Vehicle Collection</h2>

<%= link_to "Add new inventory", new_inventory_path %>

<div id="inventories">
  <% @inventories.each do |inventory| %>
    <div class='card'>
      <%= render inventory %>
      <p>
        <%= link_to "View", inventory %>
      </p>
    </div>
  <% end %>
</div>
